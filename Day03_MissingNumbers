class Solution {
    public static int  missingNumber(int[] nums) {
        int sum=0;
        for(int i=0;i<nums.length;i++){
            sum=sum+nums[i];
        }
        int act=(nums.length*(nums.length+1))/2;
         return act-sum;
    }
public static void main(String[]args){
int nums[]={3,0,1};
System.out.println(missingNumber(nums));
}
}
